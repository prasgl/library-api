{
  "name": "express-api-template",
  "version": "1.0.0",
  "description": "Template for starting a new nodejs express API project",
  "main": "app.js",
  "scripts": {
    "test-unit": "mocha -r dotenv/config 'test/*/*.js'",
    "test-grep": "mocha -r dotenv/config 'test/**/*.js' --timeout 30000 --grep",
    "start": "nodemon -r dotenv/config --max-old-space-size=2048 app.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:prasgl/express-api-template.git"
  },
  "keywords": [
    "node",
    "restful",
    "api"
  ],
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.4.0",
    "jwt-decode": "^3.1.2",
    "log4js": "^6.4.0",
    "mongodb": "^4.8.0",
    "morgan": "^1.9.1",
    "passport": "^0.4.1",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "chai-http": "4.3.0",
    "mocha": "^7.2.0",
    "nodemon": "^2.0.15",
    "sinon": "^9.2.4",
    "sinon-chai": "^3.7.0",
    "sinon-express-mock": "^2.2.1"
  },
  "engines": {
    "node": ">=14.17.0"
  }
}
